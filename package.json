{
  "name": "MyReads",
  "version": "0.0.1",
  "description": "Front end for MyReads project",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "debug": "npm run prod && npm i webpack-dev-server && npm run web --production",
    "dev": "rm -rf node_modules && npm i && npm run linter && npm run build",
    "linter": "eslint . --ext .jsx --ext .js",
    "prod": "rm -rf node_modules && npm i --production && npm run build --production",
    "start": "npm i && npm run build && npm run web",
    "web": "webpack-dev-server --content-base ./public --hot --inline --open",
    "watch": "webpack -d --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drodeur/reactnd-myreads.git"
  },
  "author": "Frédéric Bouchard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drodeur/reactnd-myreads/issues"
  },
  "homepage": "https://github.com/drodeur/reactnd-myreads#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
